  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-static-gzip": "^1.1.3",
    "faker": "^4.1.0",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "newrelic": "^5.11.0",
    "nodemon": "^1.19.1",
    "sequelize": "^5.15.1"
  },
  "devDependencies": {
