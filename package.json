    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "brotli-gzip-webpack-plugin": "^0.5.0",
    "css-loader": "^3.1.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "pg": "^7.11.0",
